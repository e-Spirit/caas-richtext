{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "lib": ["dom", "es2017"],
    "strict": true,
    "noImplicitThis": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}